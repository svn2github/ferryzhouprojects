<?xml version="1.0" encoding="gb2312"?>
<VisualStudioPropertySheet
	ProjectType="Visual C++"
	Version="8.00"
	Name="using_cppunit"
	>
	<Tool
		Name="VCCLCompilerTool"
		AdditionalIncludeDirectories="$(CPPUNIT)include"
	/>
	<Tool
		Name="VCLinkerTool"
		AdditionalDependencies="cppunitd.lib"
		AdditionalLibraryDirectories="$(CPPUNIT)lib"
	/>
	<UserMacro
		Name="CPPUNIT"
		Value="$(SolutionDir)..\third_party\cppunit\"
	/>
</VisualStudioPropertySheet>
